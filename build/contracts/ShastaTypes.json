  "source": "pragma solidity ^0.4.24;\n\n/** @title ShastaTypes */\n\n/** Internal Shasta types, for code reuse */\nlibrary ShastaTypes {\n  \n  struct EnergyContract {\n    address tokenAddress; // If tokenAddress 0x00 == ETHER as payment\n    address seller;\n    address consumer;\n    uint256 price;\n    uint monthlyWh;\n    bool enabled;\n    string ipfsContractMetadata;\n  }\n\n  struct Bill {\n    uint256 whConsumed;\n    address tokenAddress; // If tokenAddress 0x00 == ETHER as payment\n    address seller;\n    address consumer;\n    uint256 price;\n    uint256 amount;\n    string ipfsMetadata;\n  }\n\n}",
